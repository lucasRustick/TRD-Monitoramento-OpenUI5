{
	"compilerOptions": {
		"target": "es2022",
		"module": "es2022",
		"moduleResolution": "node",
		"skipLibCheck": true,
		"allowJs": true,
		"strict": true,
		"strictNullChecks": false,
		"strictPropertyInitialization": false,
		"outDir": "./dist",
		"rootDir": "./webapp",
		"types": [
			"@openui5/types",
			"@types/qunit",
		],
		"paths": {
			"bti_framework/*": ["./webapp/*"],
			"unit/*": [
				"./webapp/test/unit/*"
			],
		}
	},
	"include": [
		"./webapp/**/*"
	]
}